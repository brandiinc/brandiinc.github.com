                    <span class="k">return</span> <span class="nx">productCount</span> <span class="o">===</span> <span class="k">this</span><span class="p">.</span><span class="nx">$store</span><span class="p">.</span><span class="nx">state</span><span class="p">.</span><span class="nx">product</span><span class="p">.</span><span class="nx">checkedProductIds</span><span class="p">.</span><span class="nx">length</span>
                <span class="p">},</span>
